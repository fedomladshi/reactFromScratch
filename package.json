    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.2.4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
